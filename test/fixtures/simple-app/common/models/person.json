{
  "name": "Person",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "name": {
      "type": "string"
    },
    "status": {
      "type": "string"
    },
    "role": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {
    "products": {
      "type": "hasMany",
      "model": "Product",
      "foreignKey": "personId",
      "description": "Returns a list of products associated with a person."
    }
  },
  "methods": {},
  "mixins": {
    "ReadOnly": {
      "fields": ["status", "role"]
    }
  }
}
